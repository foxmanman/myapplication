<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.swntek.happyshop"
    android:installLocation="preferExternal">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:name=".application.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/CustomTheme">
        <activity
            android:name=".activity.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/CustomTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.LoginActivity"
            android:label="@string/app_name"
            android:theme="@style/CustomTheme"></activity>
        <activity
            android:name=".activity.RegistActivity"
            android:label="@string/app_name"
            android:theme="@style/CustomTheme"></activity>
        <activity
            android:name=".activity.GoodsOneActivity"
            android:label="@string/title_activity_goods_one"
            android:theme="@style/CustomTheme"></activity>
        <activity android:name=".activity.GoodsTypeActivity"></activity>
        <activity
            android:name=".activity.SearchActivity"
            android:label="@string/title_activity_search"
            android:theme="@style/CustomTheme"></activity>
        <activity
            android:name=".activity.ConfirmOrderActivity"
            android:label="@string/title_activity_confirm_buy"
            android:theme="@style/CustomTheme"></activity>
        <activity
            android:name=".activity.ConfirmCollectOrderActivity"
            android:label="@string/title_activity_confirm_buy"
            android:theme="@style/CustomTheme"></activity>
        <activity android:name=".activity.AddressActivity"></activity>
        <activity
            android:name=".activity.NewAddressActivity"
            android:label="@string/title_activity_new_address"
            android:theme="@style/CustomTheme"></activity>
        <activity
            android:name=".activity.AccountActivity"
            android:label="@string/title_activity_new_address"
            android:theme="@style/CustomTheme"></activity>
        <activity
            android:name=".activity.SettingActivity"
            android:label="@string/title_activity_new_address"
            android:theme="@style/CustomTheme"></activity>
        <activity
            android:name=".activity.NickNameActivity"
            android:label="@string/title_activity_new_address"
            android:theme="@style/CustomTheme"></activity>
        <activity
            android:name=".activity.AgeActivity"
            android:label="@string/title_activity_new_address"
            android:theme="@style/CustomTheme"></activity>
        <activity
            android:name=".activity.MyOrderActivity"
            android:label="@string/title_activity_new_address"
            android:theme="@style/CustomTheme"></activity>
        <activity
            android:name=".activity.ChangePwdActivity"
            android:label="@string/title_activity_new_address"
            android:theme="@style/CustomTheme"></activity>
        <activity
            android:name=".activity.AboutUsActivity"
            android:label="@string/title_activity_new_address"
            android:theme="@style/CustomTheme"></activity>


    </application>

</manifest>
